{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies" : "off",
        "import/prefer-default-export": "off",
        "linebreak-style": ["error", "windows"],
        "import/extensions": "off"
    }
}
